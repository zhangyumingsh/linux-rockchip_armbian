&spi0 {
	status = "disabled";
	assigned-clocks = <&cru CLK_SPI0>;
	assigned-clock-rates = <10000000>;
	pinctrl-names = "default";
	pinctrl-0 = <&spi0m2_pins>;	
	cs-gpios =<&gpio4 RK_PA4 GPIO_ACTIVE_LOW>,        
			<&gpio0 RK_PD4 GPIO_ACTIVE_LOW>,
			<&gpio4 RK_PA2 GPIO_ACTIVE_LOW>;
	can_mcp2515: can-mcp2515@0 {
		status = "disabled";
		compatible = "microchip,mcp2515";
		reg = <0>;
		interrupt-parent = <&gpio4>;
		interrupts = <RK_PA7 IRQ_TYPE_LEVEL_LOW>;
		spi-max-frequency = <10000000>;
		clocks = <&can_mcp2515_osc>;
		vdd-supply = <&vcc3v3_sys>;
		xceiver-supply = <&vcc3v3_sys>;
		pinctrl-names = "default";
		pinctrl-0 = <&mcp2515_int_pin>;
	};

	tft_35: st7789v@1 {
		status = "disabled";
		compatible = "sitronix,st7796s"; 
		reg = <1>;
		spi-max-frequency =<12500000>;
		fps =<60>;
		buswidth = <8>;
		rotate =<0>;
		width = <480>;
		height = <320>;
		bpp = <24>;
		bgr;
		regwidth = <8>;
		debug = <0x00>;	 //0x20 show fps
		txbuflen = <8192>;
		spi-cpol;
		spi-cpha;
		dc-gpios  = <&gpio3 RK_PC0 GPIO_ACTIVE_HIGH>;
	};
	
	spi_dev:spi-dev@1{
		compatible = "spidev";
		status = "disabled";
		spi-max-frequency=<10000000>;
		reg= <1>;
		spi-cpol;
		spi-cs-high;
	};
	
	tft_nv3041a: nv3041a@1 {
		status = "disabled";
		compatible = "NV3041A";  
		reg = <1>;
		spi-max-frequency =<12500000>;
		fps =<60>;
		buswidth = <8>;
		rotate =<0>;
		width = <480>;
		height = <272>;
		bpp = <24>;		
		regwidth = <8>;
		debug = <0x00>;	 
		txbuflen = <8192>;
		spi-cpol;
		spi-cpha;
		//rgb;
		bgr;
		dc-gpios  = <&gpio3 RK_PC0 GPIO_ACTIVE_HIGH>;
	};
	
};

&i2c3{
	status ="disabled";  //"okay";
	pinctrl-names = "default";
  	pinctrl-0 = <&i2c3m2_xfer>;
	tft_tp: ns2009@48 {
		compatible = "ti,tsc2007";
		status = "disabled";
		reg = <0x48>;
		ti,x-plate-ohms = <660>;
		ti,rt-thr = <3000>;
		ti,fuzzx = <32>;
		ti,fuzzy = <16>;
		i2c,ignore-nak = <1>;
	};
	
	tft_5406:ft5x06_ts@38 { /*触屏地址0x38*/
		compatible = "rockpi_ft5406", "ft5x06";
		reg = <0x38>;
		touchscreen-size-x = < 800 >;
		touchscreen-size-y = < 480 >;
		status = "disabled";
	};	
};

&i2c5 {
	status = "okay"; //"disabled";
	pinctrl-names = "default";
	clock-frequency = <100000>;
	pinctrl-0 = <&i2c5m3_xfer>;
	#address-cells = <1>;
	#size-cells = <0>;
	reg_display:backlight@45{ /*背光地址0x45*/
		compatible = "raspberrypi,7inch-touchscreen-panel-regulator";
		reg = <0x45>;
		status = "disabled";
    };
	
	ft5x06:ft5x06_ts@38 { 
		compatible = "rockpi_ft5406", "ft5x06";
		reg = <0x38>;
		touchscreen-size-x = < 800 >;
		touchscreen-size-y = < 480 >;
		status = "disabled";     
	};	
};

